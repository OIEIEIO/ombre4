#define DEF_OMB_VERSION "1.5.0"

//! git will put "#define GIT_ARCHIVE 1" on the next line inside archives. $Format:%n#define GIT_ARCHIVE 1$
#if defined(GIT_ARCHIVE)
#   define DEF_OMB_VERSION_TAG "$Format:%h$"
#   define DEF_OMB_BRANCH "unknown"
#else
#   define DEF_OMB_VERSION_TAG "@VERSIONTAG@"
#   define DEF_OMB_BRANCH "@GIT_BRANCH@"
#endif

#define DEF_OMB_RELEASE_NAME "White Shadow"
#define DEF_OMB_VERSION_FULL DEF_OMB_VERSION "-" DEF_OMB_VERSION_TAG "/" DEF_OMB_BRANCH

#include "version.h"

const char* const OMB_VERSION_TAG = DEF_OMB_VERSION_TAG;
const char* const OMB_VERSION = DEF_OMB_VERSION;
const char* const OMB_RELEASE_NAME = DEF_OMB_RELEASE_NAME;
const char* const OMB_VERSION_FULL = DEF_OMB_VERSION_FULL;
